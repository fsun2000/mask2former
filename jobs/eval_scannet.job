source activate m2f

cd $HOME/mask2former/Mask2Former
python train_net.py --config-file configs/scannet25k/semantic-segmentation/swin/maskformer2_swin_large_IN21k_384_bs16_160k_res640.yaml --eval-only MODEL.WEIGHTS output/swin_large_fullscannet/model_final.pth
